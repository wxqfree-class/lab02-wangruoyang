\begin{Verbatim}[commandchars=\\\{\}]
\PYG{n+nv}{x}\PYG{o}{=}6/3; \PYG{n+nb}{echo} \PYG{n+nv}{\PYGZdl{}x}   \PYG{c}{\PYGZsh{}x的值为6/3}
\PYG{n+nb}{declare} -i x     \PYG{c}{\PYGZsh{}声明x为整数}
\PYG{n+nb}{echo} \PYG{n+nv}{\PYGZdl{}x}          \PYG{c}{\PYGZsh{}x的值仍为6/3}
