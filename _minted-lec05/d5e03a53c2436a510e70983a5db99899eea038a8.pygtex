\PYG{n+nb}{declare} -r x     \PYG{c}{\PYGZsh{}声明x为只读变量}
\PYG{n+nv}{x}\PYG{o}{=}100
\PYG{n+nb}{declare} -p x     \PYG{c}{\PYGZsh{}显示变量x的声明}
\PYG{n+nb}{declare} -p       \PYG{c}{\PYGZsh{}显示所有变量}
